        <TextBox x:Name="tbName" HorizontalAlignment="Left" Height="40" Margin="62,143,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" />
        <Button x:Name="Create1" Content="Create" HorizontalAlignment="Left" Margin="100,216,0,0" VerticalAlignment="Top" Width="100" Height="32" Click="Create1_Click"/>

    </Grid>
</Window>
